<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>        
        <Console name="stdout" target="SYSTEM_OUT">         
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="stdout"/>
        </Root>
    </Loggers>
</Configuration>